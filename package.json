{
  "name": "payroll",
  "version": "1.5.2",
  "description": "SuperCamp Payroll",
  "main": "",
  "scripts": {
    "build": "babel src -d lib",
    "babel-watch-main": "npx babel assets/js/main.js -o assets/js/main-compiled.js --watch",
    "babel-watch-admin": "npx babel admin/main.js -o admin/main-compiled.js --watch",
    "startup": "ws && npm run babel-watch-main && npm run babel-watch-admin",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "reportTime": "cd admin && cat reportInfo.JSON | jq --raw-output --arg RIGHTNOW \"`date`\" '.creationTime |= $RIGHTNOW' > reportInfo.tmp && mv reportInfo.tmp reportInfo.JSON",
    "browser-sync": "browser-sync start --directory --server --files \"*.css, *.html, *.js\" --http",
    "development": "replace 'admin-prod' 'admin-dev' admin/backup.html && replace 'form-prod' 'form-dev' index.html",
    "production": "replace 'admin-dev' 'admin-prod' admin/backup.html && replace 'form-dev' 'form-prod' index.html",
    "document": "documentation build admin/main-compiled.js admin/error-checker-5000.js assets/js/staffmembers.js assets/js/main-compiled.js -f html -o docs --sort-order alpha --config documentation.yml -g"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QLN-SuperCamp/Payroll.git"
  },
  "author": "stavvie34",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/QLN-SuperCamp/Payroll/issues"
  },
  "homepage": "https://github.com/QLN-SuperCamp/Payroll#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.6",
    "cypress": "^3.0.2",
    "npx": "^10.2.0"
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "please-wait": "0.0.5"
  }
}
